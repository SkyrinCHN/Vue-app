dex: 0, pageCount: 1, pageSize: 6,left:true,\n    topShow:true,bottomShow:true };\n  },\n  methods: {\n    topIsShow(){\n      this.$emit('isShow',this.topShow)\n    },\n    bottomIsShow(){\n      this.$emit('bottomIsShow',this.bottomShow)\n    },\n    jumpDetails(e){\n      // 点击事件 获取点击事件里的dataset自定义属性 id 的值\n      var id = e.target.dataset.id;\n      console.log(id);\n      this.$router.push(\"/GoodsInfo/\"+id);\n    },\n    leftIsShow(){\n      this.$emit('leftIsShow',this.left);\n    },\n    getMore() {\n      this.pageIndex++;\n      var pno = this.pageIndex;\n      var ps = this.pageSize;\n      var url =\n        \"http://skyrinbyliu.applinzi.com/getGoodsList?pno=\" + pno + \"&pageSize=\" + ps;\n      this.axios.get(url).then(result => {\n        var rows = this.list.concat(result.data.data);\n        this.list = rows;\n        console.log(this.list);\n      });\n    }\n  },\n  created() {\n    this.getMore();\n    this.leftIsShow();\n    this.topIsShow();\n    this.bottomIsShow();\n  }\n};\n</script>\n<style scoped>\n/* 商品列表样式 */\n.goods-list {\n  /* 弹性布局 */\n  display: flex;\n  /* 子元素换行 */\n  flex-wrap: wrap;\n  /* 两端对齐 */\n  justify-content: space-between;\n  /* 加点内边距 */\n  padding: 4px;\n  /* 背景色 */\n  background: #ffff;\n}\n.goods-list .goods-item {\n  border-radius: 2px;\n  border: 1px solid rgb(224, 224, 224);\n  width: 49.8%;\n  /* 阴影 */\n  /* box-shadow: 0 0 10px #ccc; */\n  \n  /* 加点外边距 Y轴 4px */\n  margin: 1px 0;\n  padding: 2px;\n  display: flex;\n  /* 排列方式:垂直 */\n  flex-direction: column;\n  min-height: 150px;\n  /* 子元素两端对齐 */\n  justify-content: space-between;\n}\n.goods-list .goods-item img {\n  width: 100%;\n  /* height: 100%; */\n  height: 170px;\n}\n.goods-list .price .now {\n  color: red;\n  font-size: 16px;\n}\n.goods-list .price .old {\n  text-decoration: line-through;\n  font-size: 10px;\n}\n.goods-list .goods-item span {\n  margin: 0 5px;\n}\n.goods-list .goods-item p {\n  /* width: 100px; */\n  height: 40px;\n  /* white-space: pre-line; */\n  text-overflow: ellipsis;\n  overflow: hidden;\n  padding-left: 4px;\n  font-size: 13px;\n  color: #000;\n}\n.goods-list .goods-item .count p {\n  color: red;\n}\n.goods-list .goods-item .count p span {\n  color: red;\n  font-size: 20px;\n}\n/* .goods-list .goods-item .price{\n  justify-content: space-between;\n} */\n</style>\n\n"],"sourceRoot":"src/components/goods"}]}